{
    "extends": "airbnb",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": 0,
        "linebreak-style" : ["error", "windows"],
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/jsx-one-expression-per-line": 0,
        "indent" : 0
      }
  }